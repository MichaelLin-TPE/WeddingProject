<?xml version="1.0" encoding="utf-8"?>
<layout>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".customer.CustomerSelectionFemaleFragment">

        <ImageView
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/back"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/bg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/customer_selection_bg"
            app:layout_constraintBottom_toTopOf="@id/logo" />

        <ImageView
            android:id="@+id/bg5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="330dp"
            android:layout_marginEnd="315dp"
            android:src="@drawable/male_selection_bg4"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/bg" />


        <ImageView
            android:id="@+id/bg3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="315dp"
            android:src="@drawable/male_selection_bg2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/bg" />

        <ImageView
            android:id="@+id/bg2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:src="@drawable/male_selection_bg1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/bg" />

        <ImageView
            android:id="@+id/bg4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="375dp"
            android:layout_marginTop="170dp"
            android:src="@drawable/male_selection_bg3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/bg" />

        <ImageView
            android:id="@+id/bg6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="315dp"
            android:layout_marginTop="330dp"
            android:src="@drawable/male_selection_bg5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/bg" />

        <ImageView
            android:id="@+id/bg7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="220dp"
            android:layout_marginEnd="40dp"
            android:src="@drawable/male_selection_bg6"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@id/bg6" />




        <TextView
            android:id="@+id/college_friend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="大學\n朋友"
            android:textColor="@color/black"
            android:textSize="48sp"
            android:padding="90dp"
            app:layout_constraintBottom_toBottomOf="@id/bg2"
            app:layout_constraintLeft_toLeftOf="@id/bg2"
            app:layout_constraintRight_toRightOf="@id/bg2"
            app:layout_constraintTop_toTopOf="@id/bg2" />

        <TextView
            android:id="@+id/lineage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="天堂"
            android:textColor="@color/black"
            android:textSize="48sp"
            android:padding="90dp"
            android:layout_marginEnd="110dp"
            app:layout_constraintBottom_toBottomOf="@id/bg3"
            app:layout_constraintLeft_toLeftOf="@id/bg3"
            app:layout_constraintRight_toRightOf="@id/bg3"
            app:layout_constraintTop_toTopOf="@id/bg3" />


        <TextView
            android:id="@+id/family"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="親戚"
            android:textColor="@color/black"
            android:textSize="48sp"
            android:padding="90dp"
            android:layout_marginStart="60dp"
            app:layout_constraintBottom_toBottomOf="@id/bg4"
            app:layout_constraintLeft_toLeftOf="@id/bg4"
            app:layout_constraintRight_toRightOf="@id/bg4"
            app:layout_constraintTop_toTopOf="@id/bg4" />


        <TextView
            android:id="@+id/high_friend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="國中\n朋友"
            android:textColor="@color/black"
            android:textSize="48sp"
            android:padding="90dp"
            app:layout_constraintBottom_toBottomOf="@id/bg5"
            app:layout_constraintLeft_toLeftOf="@id/lineage"
            app:layout_constraintRight_toRightOf="@id/lineage"
            app:layout_constraintTop_toTopOf="@id/bg5" />

        <TextView
            android:id="@+id/hotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="儷夏"
            android:textColor="@color/black"
            android:textSize="48sp"
            android:padding="90dp"
            app:layout_constraintBottom_toBottomOf="@id/bg5"
            app:layout_constraintLeft_toLeftOf="@id/family"
            app:layout_constraintRight_toRightOf="@id/family"
            app:layout_constraintTop_toTopOf="@id/bg5" />

        <TextView
            android:id="@+id/company"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="同事"
            android:textColor="@color/black"
            android:textSize="48sp"
            android:padding="90dp"
            android:layout_marginStart="50dp"
            app:layout_constraintBottom_toBottomOf="@id/bg7"
            app:layout_constraintLeft_toLeftOf="@id/bg7"
            app:layout_constraintRight_toRightOf="@id/bg7"
            app:layout_constraintTop_toTopOf="@id/bg7" />


        <ImageView
            android:id="@+id/logo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/logo2"
            app:layout_constraintBottom_toBottomOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="160dp"
            android:layout_marginBottom="30dp"
            android:text="貴賓區&lt;男方"
            android:textColor="@color/black"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
